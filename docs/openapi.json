    "/v2.0/sinister/previous/{doc}": {
      "get": {
        "tags": ["Siniestros - AI"],
        "summary": "Obtener casos previos",
        "description": "Obtiene casos previos de siniestro asociados a un número de documento",
        "operationId": "getPreviousCases",
        "parameters": [
          {
            "name": "doc",
            "in": "path",
            "required": true,
            "description": "Número de documento del asegurado",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Casos previos encontrados",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
