{
  "openapi": "3.0.3",
  "info": {
    "title": "Backend Siniestros API",
    "description": "API para gestión de siniestros, colaboradores, pagos SOAT y centro de suscripciones",
    "version": "2.0.0",
    "contact": {
      "name": "Equipo de Desarrollo"
    }
  },
  "servers": [
    {
      "url": "{subdomain}/api",
      "description": "Servidor de la aplicación",
      "variables": {
        "subdomain": {
          "default": "https://api.example.com",
          "description": "Subdominio del servidor"
        }
      }
    }
  ],
  "paths": {
    "/v1.0/info": {
      "get": {
        "tags": ["Health"],
        "summary": "Verificar estado del servicio",
        "description": "Endpoint para verificar si el servicio está activo",
        "operationId": "healthCheck",
        "responses": {
          "200": {
            "description": "Servicio activo",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "UP"
                    },
                    "time": {
                      "type": "string",
                      "example": "30/12/2025 3:04:05 PM"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Servicio inactivo",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "DOWN"
                    },
                    "time": {
                      "type": "string",
                      "example": "30/12/2025 3:04:05 PM"
                    },
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/datetime": {
      "get": {
        "tags": ["Health"],
        "summary": "Obtener fecha y hora del servidor",
        "description": "Retorna la fecha y hora actual del servidor (UTC-5)",
        "operationId": "getDateTime",
        "responses": {
          "200": {
            "description": "Fecha y hora actual",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "time": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2025-12-30 15:04:05"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/photocheck": {
      "get": {
        "tags": ["Colaboradores"],
        "summary": "Buscar colaborador por código",
        "description": "Obtiene información de un colaborador según su código",
        "operationId": "getCollaborator",
        "parameters": [
          {
            "name": "code",
            "in": "query",
            "required": true,
            "description": "Código del colaborador",
            "schema": {
              "type": "string",
              "example": "C001"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Colaborador encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CollaboratorResponse"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/soat/returns": {
      "get": {
        "tags": ["SOAT"],
        "summary": "Obtener devoluciones SOAT",
        "description": "Busca devoluciones de SOAT por número de documento",
        "operationId": "getSoatReturns",
        "parameters": [
          {
            "name": "document",
            "in": "query",
            "required": true,
            "description": "Número de documento del asegurado",
            "schema": {
              "type": "string",
              "example": "12345678"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Devoluciones encontradas",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/subscription-center/{key}": {
      "get": {
        "tags": ["Centro de Suscripciones"],
        "summary": "Obtener opciones de suscripción por clave",
        "description": "Busca opciones de centro de suscripción por su clave identificadora",
        "operationId": "getSubscriptionCenter",
        "parameters": [
          {
            "name": "key",
            "in": "path",
            "required": true,
            "description": "Clave de la suscripción",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Opciones de suscripción encontradas",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/subscription-center": {
      "post": {
        "tags": ["Centro de Suscripciones"],
        "summary": "Crear opciones de suscripción",
        "description": "Crea nuevas opciones en el centro de suscripciones",
        "operationId": "createSubscriptionCenter",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["key", "option", "value"],
                "properties": {
                  "key": {
                    "type": "string",
                    "description": "Clave de la suscripción"
                  },
                  "option": {
                    "type": "string",
                    "description": "Opción de suscripción"
                  },
                  "value": {
                    "type": "string",
                    "description": "Valor de la opción"
                  },
                  "type": {
                    "type": "string",
                    "description": "Tipo de versión (v1 o v2)",
                    "default": "v1"
                  },
                  "description": {
                    "type": "string",
                    "description": "Descripción de la suscripción"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Suscripción creada exitosamente",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/sinister/payments": {
      "get": {
        "tags": ["Siniestros - Pagos"],
        "summary": "Obtener pagos de siniestro",
        "description": "Busca pagos de siniestro por número de documento",
        "operationId": "getSinisterPayments",
        "parameters": [
          {
            "name": "document",
            "in": "query",
            "required": true,
            "description": "Número de documento del asegurado",
            "schema": {
              "type": "string",
              "example": "12345678"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Pagos encontrados",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/sinister/save": {
      "post": {
        "tags": ["Siniestros - Pagos"],
        "summary": "Guardar siniestro con archivos adjuntos",
        "description": "Guarda información de un siniestro con archivos adjuntos (multipart/form-data)",
        "operationId": "saveSinister",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "required": ["sinister"],
                "properties": {
                  "sinister": {
                    "type": "string",
                    "description": "Datos del siniestro en formato JSON"
                  },
                  "attachments": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    },
                    "description": "Archivos adjuntos del siniestro"
                  },
                  "attachments_metadata": {
                    "type": "string",
                    "description": "Metadata de los archivos en formato JSON"
                  },
                  "environment": {
                    "type": "string",
                    "description": "Ambiente de ejecución"
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "X-Origin",
            "in": "header",
            "required": false,
            "description": "Origen de la petición (Web, Mobile, etc.)",
            "schema": {
              "type": "string",
              "default": "Web"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Siniestro guardado exitosamente",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "cases": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/sinister/get/{case_id}": {
      "get": {
        "tags": ["Siniestros - Pagos"],
        "summary": "Obtener siniestro por caso",
        "description": "Obtiene información de un siniestro por su número de caso",
        "operationId": "getSinisterByCase",
        "parameters": [
          {
            "name": "case_id",
            "in": "path",
            "required": true,
            "description": "Número de caso del siniestro",
            "schema": {
              "type": "string",
              "example": "CIS_98746"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Siniestro encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1.0/sinister/document": {
      "get": {
        "tags": ["Siniestros - Documentos"],
        "summary": "Descargar documento de siniestro",
        "description": "Descarga un documento asociado a un siniestro",
        "operationId": "downloadDocument",
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "required": true,
            "description": "URL del documento a descargar",
            "schema": {
              "type": "string",
              "format": "uri"
            }
          },
          {
            "name": "file",
            "in": "query",
            "required": true,
            "description": "Nombre del archivo",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Documento descargado exitosamente",
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v2.0/sinister": {
      "post": {
        "tags": ["Siniestros - AI"],
        "summary": "Guardar siniestro con procesamiento IA",
        "description": "Guarda un siniestro utilizando procesamiento de inteligencia artificial (GenAI)",
        "operationId": "saveSinisterWithAI",
        "parameters": [
          {
            "name": "X-Origin",
            "in": "header",
            "required": false,
            "description": "Origen de la petición (Web, Mobile, etc.)",
            "schema": {
              "type": "string",
              "default": "Web"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SinisterRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Siniestro guardado exitosamente",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "cases": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": ["Siniestros - AI"],
        "summary": "Obtener todos los siniestros",
        "description": "Obtiene listado de todos los siniestros procesados con IA",
        "operationId": "getAllSinisters",
        "responses": {
          "200": {
            "description": "Lista de siniestros",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v2.0/sinister/case/{case_id}": {
      "get": {
        "tags": ["Siniestros - AI"],
        "summary": "Obtener siniestro por caso (AI)",
        "description": "Obtiene información de un siniestro procesado con IA por su número de caso",
        "operationId": "getSinisterByCaseAI",
        "parameters": [
          {
            "name": "case_id",
            "in": "path",
            "required": true,
            "description": "Número de caso del siniestro",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Siniestro encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v2.0/sinister/doc": {
      "post": {
        "tags": ["Siniestros - AI"],
        "summary": "Analizar documento de siniestro",
        "description": "Analiza un documento de siniestro utilizando inteligencia artificial",
        "operationId": "analyzeDocument",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SinisterCase"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Documento analizado exitosamente",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v2.0/sinister/doc/observed": {
      "post": {
        "tags": ["Siniestros - AI"],
        "summary": "Cargar documento observado",
        "description": "Carga un documento observado y actualiza el siniestro correspondiente",
        "operationId": "uploadObservedDocument",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SinisterCase"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Documento observado actualizado correctamente",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud incorrecta",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error interno del servidor",
            "content": {
              "application/json": {
