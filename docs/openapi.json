  },
  "components": {
    "schemas": {
      "SinisterRequest": {
        "type": "object",
        "required": ["tipoPoliza", "asegurado", "solicitante", "fechaOcurrencia"],
        "properties": {
          "tipoPoliza": {
            "type": "string",
            "description": "Tipo de póliza del seguro",
            "example": "ACCIDENTES_PERSONALES"
          },
          "asegurado": {
            "$ref": "#/components/schemas/Asegurado"
          },
          "solicitante": {
            "$ref": "#/components/schemas/Solicitante"
          },
          "narracion": {
            "type": "string",
            "description": "Narración de los hechos del siniestro"
          },
          "fechaOcurrencia": {
            "type": "string",
            "format": "date",
            "description": "Fecha de ocurrencia del siniestro",
            "example": "2025-12-30"
          },
          "montoSolicitado": {
            "type": "string",
            "description": "Monto solicitado para el siniestro",
            "example": "5000.00"
          },
          "pagador": {
            "type": "string",
            "description": "Identificador del pagador"
          },
          "fechasIncapacidad": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FechasIncapacidad"
            }
          },
          "declaracionJurada": {
            "type": "boolean",
            "description": "Indica si se firmó declaración jurada",
            "default": false
          },
          "beneficiarios": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Beneficiario"
            }
          },
          "documentos": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DocumentSection"
            }
          }
        }
      },
      "Asegurado": {
        "type": "object",
        "required": ["nroDocumento"],
        "properties": {
          "apellidoPaterno": {
            "type": "string",
            "description": "Apellido paterno del asegurado",
            "example": "García"
          },
          "apellidoMaterno": {
            "type": "string",
            "description": "Apellido materno del asegurado",
            "example": "López"
          },
          "nombres": {
            "type": "string",
            "description": "Nombres del asegurado",
            "example": "Juan Carlos"
          },
          "nroDocumento": {
            "type": "string",
            "description": "Número de documento del asegurado",
            "example": "12345678"
          }
        }
      },
      "Solicitante": {
        "type": "object",
        "required": ["nroDocumento"],
        "properties": {
          "apellidoPaterno": {
            "type": "string",
            "description": "Apellido paterno del solicitante",
            "example": "Pérez"
          },
          "apellidoMaterno": {
            "type": "string",
            "description": "Apellido materno del solicitante",
            "example": "Silva"
          },
          "nombres": {
            "type": "string",
            "description": "Nombres del solicitante",
            "example": "María Elena"
          },
          "nroDocumento": {
            "type": "string",
            "description": "Número de documento del solicitante",
            "example": "87654321"
          },
          "correo": {
            "type": "string",
            "format": "email",
            "description": "Correo electrónico del solicitante",
            "example": "maria.silva@example.com"
          },
          "celular": {
            "type": "string",
            "description": "Número de celular del solicitante",
            "example": "987654321"
          }
        }
      },
      "FechasIncapacidad": {
